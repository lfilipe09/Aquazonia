{
  "name": "aquazonia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --verbose --maxWorkers=50%",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "generate": "yarn plop --plopfile generators/plopfile.js",
